  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#0000FF"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF0000"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type"],
      "settings": {
        "foreground": "#800080"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage.modifier"],
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "Actions",
      "scope": [
        "keyword.control",
        "keyword.other.template",
        "keyword.other.substitution",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#800080"
      }
    },
    {
      "name": "Markers",
      "scope": [
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#FFFF00"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#FFFF00"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#800080"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#ff0000"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "Arguments, Parameters",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#00FF00"
      }
    },
    {
      "name": "Number, Constant, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "Appreciated Pre-existing Tokens(Repect demanded!)",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
        "variable.parameter.function.language.special.self.python",
        "meta.function-call.arguments.python",
        "variable.parameter.function-call.python",
        "variable.language.special.self.python",
        "variable.parameter.function.language.python",
        "meta.item-access.arguments.python",
        "support.class.latex",
        "variable.parameter.function.latex"
      ],
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "Created Token Names(Changes would break anything)",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types",
        "punctuation.separator.element.python",
        "string.quoted.single.python",
        "entity.name.function.python"
      ],
      "settings": {
        "foreground": "#00ff00"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#ff0000"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#ff0000"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#800080"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffff00"
      }
    },
    {
      "name": "entity.name.00ff00.js",
      "scope": ["entity.name.00ff00.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "meta.00ff00.js",
      "scope": [
        "meta.class-00ff00.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#00FF00"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
